<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.Debug2Activity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#F8373737"
        android:longClickable="false"
        android:measureAllChildren="false"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="82dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TestTitle"
                android:textAllCaps="false"
                android:textColor="#ffffffff"
                android:textSize="22sp" />

            <Button
                android:id="@+id/debug2_updateChecker_changeAppBuild"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginEnd="5sp"
                android:layout_marginRight="5sp"
                android:alpha="0.8"
                android:layerType="none"
                android:text="TestButton"
                android:textAllCaps="false"
                android:textColor="#ffffff"
                android:textSize="15sp"
                app:backgroundTint="#232323"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Services"
                android:textAllCaps="false"
                android:textColor="#ffffffff"
                android:textSize="22sp" />

            <Button
                android:id="@+id/debug2_servicesStartWidgetUpdater"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginEnd="5sp"
                android:layout_marginRight="5sp"
                android:alpha="0.8"
                android:layerType="none"
                android:text="start WidgetsUpdaterService"
                android:textAllCaps="false"
                android:textColor="#ffffff"
                android:textSize="15sp"
                app:backgroundTint="#232323"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/debug2_servicesStopWidgetUpdater"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginEnd="5sp"
                android:layout_marginRight="5sp"
                android:alpha="0.8"
                android:layerType="none"
                android:text="stop WidgetsUpdaterService"
                android:textAllCaps="false"
                android:textColor="#ffffff"
                android:textSize="15sp"
                app:backgroundTint="#232323"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/debug2_servicesStartedList"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginEnd="5sp"
                android:layout_marginRight="5sp"
                android:alpha="0.8"
                android:layerType="none"
                android:text="started list"
                android:textAllCaps="false"
                android:textColor="#ffffff"
                android:textSize="15sp"
                app:backgroundTint="#232323"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Widgets"
                android:textAllCaps="false"
                android:textColor="#ffffffff"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="   data"
                android:textAllCaps="false"
                android:textColor="#ffffffff"
                android:textSize="22sp" />

            <Button
                android:id="@+id/debug2_widgetsDataJSONFILE"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginEnd="5sp"
                android:layout_marginRight="5sp"
                android:alpha="0.8"
                android:layerType="none"
                android:text="json (widgets.json)"
                android:textAllCaps="false"
                android:textColor="#ffffff"
                android:textSize="15sp"
                app:backgroundTint="#232323"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/debug2_widgetsDataSave"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginEnd="5sp"
                android:layout_marginRight="5sp"
                android:alpha="0.8"
                android:layerType="none"
                android:text="save"
                android:textAllCaps="false"
                android:textColor="#ffffff"
                android:textSize="15sp"
                app:backgroundTint="#232323"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/debug2_widgetsDataLoad"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginEnd="5sp"
                android:layout_marginRight="5sp"
                android:alpha="0.8"
                android:layerType="none"
                android:text="load"
                android:textAllCaps="false"
                android:textColor="#ffffff"
                android:textSize="15sp"
                app:backgroundTint="#232323"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/debug2_widgetsDataViewJavaVars"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginEnd="5sp"
                android:layout_marginRight="5sp"
                android:alpha="0.8"
                android:layerType="none"
                android:text="WidgetsData.java vars"
                android:textAllCaps="false"
                android:textColor="#ffffff"
                android:textSize="15sp"
                app:backgroundTint="#232323"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/debug2_widgetsDataVariablesText"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="debug2_widgetsDataVariablesText"
                android:textColor="#B3B0B0" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="   Manager"
                android:textAllCaps="false"
                android:textColor="#ffffffff"
                android:textSize="22sp" />

            <Button
                android:id="@+id/debug2_widgetsManagerAdd"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginEnd="5sp"
                android:layout_marginRight="5sp"
                android:alpha="0.8"
                android:layerType="none"
                android:text="addWidget"
                android:textAllCaps="false"
                android:textColor="#ffffff"
                android:textSize="15sp"
                app:backgroundTint="#232323"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/debug2_widgetsManagerRemove"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginEnd="5sp"
                android:layout_marginRight="5sp"
                android:alpha="0.8"
                android:layerType="none"
                android:text="removeWidget"
                android:textAllCaps="false"
                android:textColor="#ffffff"
                android:textSize="15sp"
                app:backgroundTint="#232323"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/debug2_widgetsManagerIsExist"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginEnd="5sp"
                android:layout_marginRight="5sp"
                android:alpha="0.8"
                android:layerType="none"
                android:text="isWidgetExist"
                android:textAllCaps="false"
                android:textColor="#ffffff"
                android:textSize="15sp"
                app:backgroundTint="#232323"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/debug2_widgetsManagerGetById"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginEnd="5sp"
                android:layout_marginRight="5sp"
                android:alpha="0.8"
                android:layerType="none"
                android:text="getWidgetById"
                android:textAllCaps="false"
                android:textColor="#ffffff"
                android:textSize="15sp"
                app:backgroundTint="#232323"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Update Checker"
                android:textAllCaps="false"
                android:textColor="#ffffffff"
                android:textSize="22sp" />

            <Button
                android:id="@+id/debug2_tupdateChecker_getVersion"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginEnd="5sp"
                android:layout_marginRight="5sp"
                android:alpha="0.8"
                android:layerType="none"
                android:text="getVersion"
                android:textAllCaps="false"
                android:textColor="#ffffff"
                android:textSize="15sp"
                app:backgroundTint="#232323"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Unknown"
                android:textAllCaps="false"
                android:textColor="#ffffffff"
                android:textSize="22sp" />

            <Button
                android:id="@+id/debug2OpenActivity"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="5sp"
                android:layout_marginLeft="5sp"
                android:layout_marginEnd="5sp"
                android:layout_marginRight="5sp"
                android:alpha="0.8"
                android:layerType="none"
                android:text="OPen Activity"
                android:textAllCaps="false"
                android:textColor="#ffffff"
                android:textSize="15sp"
                app:backgroundTint="#232323"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </LinearLayout>
